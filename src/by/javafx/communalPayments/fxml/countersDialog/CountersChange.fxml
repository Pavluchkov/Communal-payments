<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.*?>
<?import javafx.scene.effect.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>
<AnchorPane xmlns:fx="http://javafx.com/fxml/1" prefHeight="305.0" prefWidth="400.0" xmlns="http://javafx.com/javafx/8">
    <children>
        <HBox alignment="CENTER" layoutY="6.0" prefHeight="40.0" prefWidth="360.0" AnchorPane.leftAnchor="20.0"
              AnchorPane.rightAnchor="20.0" AnchorPane.topAnchor="10.0">
            <children>
                <Label text="Параметры счетчика">
                    <font>
                        <Font name="Arial Bold Italic" size="18.0"/>
                    </font>
                </Label>
            </children>
        </HBox>
        <HBox alignment="CENTER_LEFT" layoutY="60.0" prefHeight="30.0" prefWidth="360.0" AnchorPane.leftAnchor="20.0"
              AnchorPane.rightAnchor="20.0" AnchorPane.topAnchor="60.0">
            <children>
                <Label prefHeight="30.0" prefWidth="155.0" text="Объект учета">
                    <font>
                        <Font name="Arial Italic" size="14.0"/>
                    </font>
                </Label>
                <ComboBox fx:id="objectCombo" prefHeight="30.0" prefWidth="206.0">
                    <effect>
                        <InnerShadow height="10.0" radius="4.5" width="10.0"/>
                    </effect>
                </ComboBox>
            </children>
        </HBox>
        <HBox alignment="CENTER_LEFT" layoutX="4.0" layoutY="90.0" prefHeight="30.0" prefWidth="360.0"
              AnchorPane.leftAnchor="20.0" AnchorPane.rightAnchor="20.0" AnchorPane.topAnchor="100.0">
            <children>
                <Label prefHeight="30.0" prefWidth="155.0" text="Услуга">
                    <font>
                        <Font name="Arial Italic" size="14.0"/>
                    </font>
                    <HBox.margin>
                        <Insets/>
                    </HBox.margin>
                </Label>
                <ComboBox fx:id="serviceCombo" prefHeight="30.0" prefWidth="206.0">
                    <effect>
                        <InnerShadow height="10.0" radius="4.5" width="10.0"/>
                    </effect>
                </ComboBox>
            </children>
        </HBox>
        <HBox alignment="CENTER_RIGHT" layoutX="12.0" layoutY="233.0" prefHeight="30.0" prefWidth="360.0"
              AnchorPane.leftAnchor="20.0" AnchorPane.rightAnchor="20.0" AnchorPane.topAnchor="250.0">
            <children>
                <Button mnemonicParsing="false" onAction="#btnOkClicked" prefHeight="30.0" prefWidth="110.0"
                        text="Подтвердить">
                    <font>
                        <Font name="Arial" size="14.0"/>
                    </font>
                    <effect>
                        <InnerShadow color="#08a12e80"/>
                    </effect>
                </Button>
                <Button fx:id="btnCancel" layoutX="180.0" layoutY="17.0" mnemonicParsing="false"
                        onAction="#btnCancelClicked" prefHeight="30.0" prefWidth="110.0" text="Отмена">
                    <HBox.margin>
                        <Insets left="30.0"/>
                    </HBox.margin>
                    <font>
                        <Font name="Arial" size="14.0"/>
                    </font>
                    <effect>
                        <InnerShadow color="#d90d0d80"/>
                    </effect>
                </Button>
            </children>
            <effect>
                <DropShadow height="10.0" radius="4.5" width="10.0"/>
            </effect>
        </HBox>
        <HBox alignment="CENTER_LEFT" layoutX="30.0" layoutY="110.0" prefHeight="30.0" prefWidth="360.0"
              AnchorPane.leftAnchor="20.0" AnchorPane.rightAnchor="20.0" AnchorPane.topAnchor="140.0">
            <children>
                <Label prefHeight="30.0" prefWidth="155.0" text="Название счетчика">
                    <font>
                        <Font name="Arial Italic" size="14.0"/>
                    </font>
                    <HBox.margin>
                        <Insets/>
                    </HBox.margin>
                </Label>
                <TextField fx:id="nameField" prefHeight="30.0" prefWidth="206.0">
                    <font>
                        <Font name="Arial" size="14.0"/>
                    </font>
                    <effect>
                        <InnerShadow height="10.0" radius="4.5" width="10.0"/>
                    </effect>
                </TextField>
            </children>
        </HBox>
        <HBox alignment="CENTER_LEFT" layoutX="20.0" layoutY="181.0" prefHeight="30.0" prefWidth="360.0"
              AnchorPane.leftAnchor="20.0" AnchorPane.rightAnchor="20.0" AnchorPane.topAnchor="180.0">
            <children>
                <Label fx:id="measureLabel" prefHeight="30.0" prefWidth="155.0" text="Начальные показания">
                    <font>
                        <Font name="Arial Italic" size="14.0"/>
                    </font>
                    <HBox.margin>
                        <Insets/>
                    </HBox.margin>
                </Label>
                <TextField fx:id="recentField" prefHeight="30.0" prefWidth="206.0">
                    <font>
                        <Font name="Arial" size="14.0"/>
                    </font>
                    <effect>
                        <InnerShadow height="10.0" radius="4.5" width="10.0"/>
                    </effect>
                </TextField>
            </children>
        </HBox>
    </children>
    <effect>
        <InnerShadow height="15.0" radius="7.0" width="15.0"/>
    </effect>
</AnchorPane>
