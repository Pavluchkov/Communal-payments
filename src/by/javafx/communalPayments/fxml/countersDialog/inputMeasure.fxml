<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>
<AnchorPane xmlns:fx="http://javafx.com/fxml/1" prefHeight="255.0" prefWidth="430.0" xmlns="http://javafx.com/javafx/8">
    <children>
        <HBox alignment="CENTER" layoutY="6.0" prefHeight="40.0" prefWidth="600.0" AnchorPane.leftAnchor="0.0"
              AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="10.0">
            <children>
                <Label fx:id="textLabel">
                    <font>
                        <Font name="Arial Bold Italic" size="18.0"/>
                    </font>
                </Label>
            </children>
        </HBox>
        <HBox alignment="CENTER_RIGHT" layoutX="12.0" layoutY="233.0" prefHeight="30.0" prefWidth="521.0"
              AnchorPane.leftAnchor="20.0" AnchorPane.rightAnchor="20.0" AnchorPane.topAnchor="200.0">
            <children>
                <Button mnemonicParsing="false" onAction="#btnOkClicked" prefHeight="30.0" prefWidth="110.0"
                        text="Подтвердить">
                    <font>
                        <Font name="Arial" size="14.0"/>
                    </font>
                </Button>
                <Button fx:id="btnCancel" layoutX="180.0" layoutY="17.0" mnemonicParsing="false"
                        onAction="#btnCancelClicked" prefHeight="30.0" prefWidth="110.0" text="Отмена">
                    <HBox.margin>
                        <Insets left="30.0"/>
                    </HBox.margin>
                    <font>
                        <Font name="Arial" size="14.0"/>
                    </font>
                </Button>
            </children>
        </HBox>
        <HBox alignment="CENTER_LEFT" layoutX="30.0" layoutY="110.0" prefHeight="30.0" prefWidth="477.0"
              AnchorPane.leftAnchor="20.0" AnchorPane.rightAnchor="20.0" AnchorPane.topAnchor="100.0">
            <children>
                <Label prefHeight="30.0" prefWidth="170.0" text="Текущие показания">
                    <font>
                        <Font name="Arial Italic" size="14.0"/>
                    </font>
                    <HBox.margin>
                        <Insets/>
                    </HBox.margin>
                </Label>
                <TextField fx:id="measureField" prefHeight="30.0" prefWidth="220.0">
                    <font>
                        <Font name="Arial" size="14.0"/>
                    </font>
                </TextField>
            </children>
        </HBox>
        <HBox alignment="CENTER_LEFT" layoutX="20.0" layoutY="184.0" prefHeight="30.0" prefWidth="477.0"
              AnchorPane.leftAnchor="20.0" AnchorPane.rightAnchor="20.0" AnchorPane.topAnchor="140.0">
            <children>
                <Label prefHeight="30.0" prefWidth="170.0" text="Дата">
                    <font>
                        <Font name="Arial Italic" size="14.0"/>
                    </font>
                    <HBox.margin>
                        <Insets/>
                    </HBox.margin>
                </Label>
                <DatePicker fx:id="datePicker" prefHeight="30.0" prefWidth="220.0"/>
            </children>
        </HBox>
        <HBox alignment="CENTER_LEFT" layoutX="20.0" layoutY="220.0" prefHeight="30.0" prefWidth="477.0"
              AnchorPane.leftAnchor="20.0" AnchorPane.rightAnchor="20.0" AnchorPane.topAnchor="60.0">
            <children>
                <Label prefHeight="30.0" prefWidth="170.0" text="Предыдущие показания">
                    <font>
                        <Font name="Arial Italic" size="14.0"/>
                    </font>
                    <HBox.margin>
                        <Insets/>
                    </HBox.margin>
                </Label>
                <TextField fx:id="previousMeasureField" prefHeight="30.0" prefWidth="220.0">
                    <font>
                        <Font name="Arial" size="14.0"/>
                    </font>
                </TextField>
            </children>
        </HBox>
    </children>
</AnchorPane>
