                        <Font name="Arial" size="14.0" />
                    </font>
               <effect>
                  <InnerShadow color="#08a12e80" />
               </effect>
                </Button>
                <Button fx:id="btnCancel" layoutX="180.0" layoutY="17.0" mnemonicParsing="false" onAction="#btnCancelClicked" prefHeight="30.0" prefWidth="110.0" text="Отмена">
