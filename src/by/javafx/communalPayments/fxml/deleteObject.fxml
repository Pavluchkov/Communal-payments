            </children>
        </HBox>
        <HBox alignment="CENTER_LEFT" layoutX="8.0" layoutY="90.0" prefHeight="30.0" prefWidth="340.0" AnchorPane.leftAnchor="20.0" AnchorPane.rightAnchor="20.0" AnchorPane.topAnchor="50.0">
            <children>
                <Label fx:id="nameDeleteObject" alignment="CENTER" contentDisplay="CENTER" prefHeight="20.0" prefWidth="477.0" text="test">
                    <font>
                        <Font name="Arial Italic" size="14.0" />
                    </font>
                </Label>
            </children>
        </HBox>
        <HBox alignment="CENTER_LEFT" layoutX="20.0" layoutY="50.0" prefHeight="30.0" prefWidth="340.0" AnchorPane.leftAnchor="20.0" AnchorPane.rightAnchor="20.0" AnchorPane.topAnchor="85.0">
