<AnchorPane prefHeight="190.0" prefWidth="380.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1">
    <children>
        <HBox alignment="CENTER" layoutY="6.0" prefHeight="40.0" prefWidth="340.0" AnchorPane.leftAnchor="20.0" AnchorPane.rightAnchor="20.0" AnchorPane.topAnchor="10.0">
            <children>
                <Label text="Удаление объекта">
                    <font>
                        <Font name="Arial Bold Italic" size="18.0" />
